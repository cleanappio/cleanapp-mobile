    "@react-navigation/stack": "^7.4.7",
    "axios": "^1.8.1",
    "babel-loader": "^10.0.0",
    "ethers": "^6.13.5",
    "i18next": "24.2.2",
    "react": "19.0.0",
    "react-i18next": "^15.4.1",
    "react-native": "0.78.2",
    "react-native-config": "^1.5.5",
